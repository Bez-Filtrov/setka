{
	"log": {
		"access": {
			"type": "Console",
			"level": "Debug"
		},
		"error": {
			"type": "Console",
			"level": "Debug"
		}
	},
	"inbounds": [
		{
			"tag": "trojan",
			"listen": "0.0.0.0",
			"port": 443,
			"protocol": "#v2ray.core.proxy.trojan.ServerConfig",
			"settings": {
				"users": [
					{
						"email": "user",
						"account": {
							"@type": "v2ray.core.proxy.trojan.Account",
							"password": "${UUID}"
						}
					}
				],
				"packetEncoding": "Packet"
			},
			"streamSettings": {
				"transport": "tlsmirror",
				"transportSettings": {
					"primaryKey": "${PRIMARY_KEY}",
					"forwardTag": "direct",
					"forwardAddress": "nginx",
					"forwardPort": 443,
					"sequenceWatermarkingEnabled": true
				}
			}
		}
	],
	"outbounds": [
		{
			"protocol": "freedom",
			"tag": "direct"
		}
	]
}